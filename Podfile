  pod 'FacebookCore'
  pod 'FacebookLogin'
  
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  
  pod 'SwiftDate'
  pod 'SwiftRichString'
  
  pod 'NotificationBannerSwift'
  pod 'SwipeCellKit'
  pod 'KRProgressHUD'
  
  pod 'SCLAlertView'
  pod 'PMAlertController'
  pod 'CDAlertView'
  
  pod 'JSQMessagesViewController'
  
  pod 'SideMenu'
  
  pod 'Eureka' #, :git => 'https://github.com/xmartlabs/Eureka.git', :branch => 'feature/Xcode9-Swift4'
  
  pod 'Alamofire'
  pod 'SDWebImage'
  pod 'ImageLoader'
  
  pod 'SwiftyUserDefaults'
  
  pod 'TimeZoneLocate'

  target 'peacaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'peacaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

